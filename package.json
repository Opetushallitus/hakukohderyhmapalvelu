{
  "scripts": {
    "cypress:open": "cypress open -C oph-configuration/cypress.local-environment.json",
    "cypress:run:local-environment": "cypress run -C oph-configuration/cypress.local-environment.json --headless",
    "cypress:run:travis": "cypress run -C oph-configuration/cypress.travis.json --headless",
    "lint:clj": "clj-kondo --config oph-configuration/clj-kondo.config.edn --lint src",
    "lint:clj:lint-staged": "clj-kondo --config oph-configuration/clj-kondo.config.edn --lint",
    "lint:js": "eslint --ignore-path \".gitignore\" --ext .js \"cypress\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "npm run lint:js"
    ],
    "src/**/*.{clj,cljs,cljc}": [
      "npm run lint:clj:lint-staged"
    ]
  },
  "devDependencies": {
    "clj-kondo": "2020.1.13",
    "cypress": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.9",
    "prettier": "2.0.2",
    "shadow-cljs": "2.8.83"
  },
  "dependencies": {
    "highlight.js": "9.15.10",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-highlight.js": "1.0.7"
  }
}
